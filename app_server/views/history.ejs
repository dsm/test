<div style="margin: 30px auto 10px auto">
    <a href="/history/deleteUser/" class="btn btn-danger btn-sm active" role="button" aria-pressed="true">Delete All History</a>
</div>
<div class="container" style="margin: 5px auto 100px auto">
    <div class="row" style="margin-top: 5px">
        <table id="table" class="table table-striped table-bordered table-sm" cellspacing="0" width="100%">
            <thead class="thead-dark" align="center">
                <tr>
                    <th class="th-sm" scope="col">Name
                    </th>
                    <th class="th-sm" scope="col">Surname
                    </th>
                    <th class="th-sm" scope="col">Cardid
                    </th>
                    <th class="th-sm" scope="col">Date
                    </th>
                    <th class="th-sm" scope="col">Status
                </tr>
            </thead>
            <tbody id="mytab" align="center">
                <%for(var i= 0; i<History.length; i++){%>
                    <tr>
                        <td id="td1">
                            <%=History[i].name%>
                        </td>
                        <td id="td2">
                            <%=History[i].surname%>
                        </td>
                        <td id="td3">
                            <%=History[i].cardid%>
                        </td>
                        <td id="td4">
                            <%=History[i].date%>
                        </td>
                        <td id="td5">
                            <%=History[i].status%>
                        </td>
                    </tr>
                    <%}%>

            </tbody>
        </table>
    </div>
</div>